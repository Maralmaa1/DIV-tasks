        "import time\n",
        "start = time.time()\n",
        "##### \n",
        "folded_thickness = thickness * 2 ** 43\n",
        "#####\n",
        "elapsed_time = time.time() - start\n",
        "print(\"time : {}[s]\".format(elapsed_time))"
